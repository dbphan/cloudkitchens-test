# Cloud Kitchens API Configuration
# Copy this file to .env and fill in your actual values

# Authentication token for Cloud Kitchens API (required)
AUTH_TOKEN=your_auth_token_here

# API endpoint (optional, defaults to https://api.cloudkitchens.com)
ENDPOINT=https://api.cloudkitchens.com

# Problem configuration (optional)
PROBLEM_NAME=
PROBLEM_SEED=

# Timing configuration (optional)
# Rate: Inverse order rate (e.g., 500ms)
# Min: Minimum pickup time (e.g., 4s)
# Max: Maximum pickup time (e.g., 8s)
RATE=500ms
MIN=4s
MAX=8s
